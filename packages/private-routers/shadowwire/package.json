{
  "name": "@privacy-router-sdk/shadowwire",
  "version": "0.1.0",
  "description": "ShadowWire provider for privacy router - private payments using Bulletproof ZK proofs",
  "author": "",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run typecheck && pnpm run test && pnpm run build"
  },
  "keywords": [
    "privacy",
    "shadowwire",
    "solana",
    "bulletproofs",
    "zk",
    "typescript"
  ],
  "dependencies": {
    "@privacy-router-sdk/private-routers-core": "workspace:*",
    "@privacy-router-sdk/signers-core": "workspace:*",
    "@radr/shadowwire": "^1.1.15",
    "@solana/web3.js": "^1.95.8"
  }
}
